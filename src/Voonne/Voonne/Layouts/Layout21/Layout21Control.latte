<div class="col-xs-12 col-md-8 col-lg-9">
	{foreach $elements[\Voonne\Voonne\Content\ContentManager::POSITION_LEFT] as $index => $element}
		<div class="panel">
			<div class="panel-header">
				<h3>Title</h3>
			</div>
			<div class="panel-body">
				{control 'element_left_' . $index}
			</div>
		</div>
	{/foreach}
</div>
<div class="col-xs-12 col-md-4 col-lg-3">
	{foreach $elements[\Voonne\Voonne\Content\ContentManager::POSITION_RIGHT] as $index => $element}
		<div class="panel">
			<div class="panel-header">
				<h3>Title</h3>
			</div>
			<div class="panel-body">
				{control 'element_right_' . $index}
			</div>
		</div>
	{/foreach}
</div>
